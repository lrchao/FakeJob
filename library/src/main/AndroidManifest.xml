<manifest package="com.lrchao.fakejob"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:supportsRtl="true"
        android:theme="@style/JobAppTheme"
        >

        <activity android:name=".ui.activity.main.MainActivity"/>
        <activity android:name=".ui.activity.category.CategoryJobListActivity"/>
        <activity android:name=".ui.activity.job.JobDetailsActivity"/>
        <activity android:name=".ui.activity.job.MyJobListActivity"/>
        <activity android:name=".ui.activity.login.LoginActivity"/>
        <activity android:name=".ui.activity.register.RegisterActivity"/>
        <activity android:name=".ui.activity.favorites.MyFavoritesListActivity"/>
        <activity android:name=".ui.activity.location.LocationActivity"/>
        <activity android:name=".ui.activity.search.SearchActivity"/>

        <service
            android:name=".service.RequestService"
            android:exported="false"/>

    </application>

</manifest>
